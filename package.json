{
  "name": "notes",
  "version": "0.2.0",
  "description": "File-storage backed notes in markdown",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p data/build/js && cp node_modules/autosize/dist/autosize.min.js data/build/js/ && rollup -o data/build/js/bundle.js -f iife -m inline src/js/index.js",
    "develop": "node src/server.js --dev",
    "pre-develop": "npm run build",
    "sync": "node -e \"require('./src/dropbox.js').dropboxSync()\"",
    "hash": "node -e \"console.log(require('./src/dropbox.js').dropboxHash(process.argv[1]))\"",
    "serve": "npm run build && npm run sync && node src/server.js --production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Flaki @ Skylark",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-ee/notes.skylark.ee.git"
  },
  "dependencies": {
    "autosize": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "dropbox": "^2.5.13",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "ppc-messaging": "^2.0.0",
    "rollup": "^0.53.4"
  }
}
